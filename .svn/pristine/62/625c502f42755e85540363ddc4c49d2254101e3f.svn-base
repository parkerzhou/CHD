package com.glorisun.umm.pojo;
public class TUser{
	private int us_id;          //主键
	private String us_userId;   //用户ID
	private String us_userName; //用户名称
	private String us_nickName; //登陆ＩＤ
	private String us_userPwd;  //密码
	private String us_email;    //邮箱
	private String us_mobileNo; //手机号
	private int ut_id;          //用户类型
	private int us_state;       //用户状态
	private int us_dataFrom;    //数据来源 1:手动 2：被动
	private int us_defLanguage; //缺省语言
	public int getUs_id() {
		return us_id;
	}
	public void setUs_id(int us_id) {
		this.us_id = us_id;
	}
	public String getUs_userId() {
		return us_userId;
	}
	public void setUs_userId(String us_userId) {
		this.us_userId = us_userId;
	}
	public String getUs_userName() {
		return us_userName;
	}
	public void setUs_userName(String us_userName) {
		this.us_userName = us_userName;
	}
	public String getUs_nickName() {
		return us_nickName;
	}
	public void setUs_nickName(String us_nickName) {
		this.us_nickName = us_nickName;
	}
	public String getUs_userPwd() {
		return us_userPwd;
	}
	public void setUs_userPwd(String us_userPwd) {
		this.us_userPwd = us_userPwd;
	}
	public String getUs_email() {
		return us_email;
	}
	public void setUs_email(String us_email) {
		this.us_email = us_email;
	}
	public String getUs_mobileNo() {
		return us_mobileNo;
	}
	public void setUs_mobileNo(String us_mobileNo) {
		this.us_mobileNo = us_mobileNo;
	}
	public int getUt_id() {
		return ut_id;
	}
	public void setUt_id(int ut_id) {
		this.ut_id = ut_id;
	}
	public int getUs_state() {
		return us_state;
	}
	public void setUs_state(int us_state) {
		this.us_state = us_state;
	}
	public int getUs_dataFrom() {
		return us_dataFrom;
	}
	public void setUs_dataFrom(int us_dataFrom) {
		this.us_dataFrom = us_dataFrom;
	}
	public int getUs_defLanguage() {
		return us_defLanguage;
	}
	public void setUs_defLanguage(int us_defLanguage) {
		this.us_defLanguage = us_defLanguage;
	}
	
}