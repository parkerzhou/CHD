package com.glorisun.umm.pojo;

/*
 * 角色实体类
 * 用与保存角色信息
 * @author Zero
 */
public class Role {
	//角色id
	private String rl_id;
	
	//角色代码usercode
	private String rl_code;
	
	//角色名称username
	private String rl_name;
	
	//适用范围range
	private String rl_range;
	
	//排序orderSeq
	private int rl_orderSeq;

	
	//setter and getter
	
	public String getRl_id() {
		return rl_id;
	}

	public void setRl_id(String rl_id) {
		this.rl_id = rl_id;
	}

	public String getRl_code() {
		return rl_code;
	}

	public void setRl_code(String rl_code) {
		this.rl_code = rl_code;
	}

	public String getRl_name() {
		return rl_name;
	}

	public void setRl_name(String rl_name) {
		this.rl_name = rl_name;
	}

	public String getRl_range() {
		return rl_range;
	}

	public void setRl_range(String rl_range) {
		this.rl_range = rl_range;
	}

	public int getRl_orderSeq() {
		return rl_orderSeq;
	}

	public void setRl_orderSeq(int rl_orderSeq) {
		this.rl_orderSeq = rl_orderSeq;
	}

	


	

}
